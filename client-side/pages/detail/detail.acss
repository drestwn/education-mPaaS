#main-container {
min-height: 99.5vh;
min-width: 100vw;
padding-top: 5px;
background-size: cover;
}
.card {
  background-color: #f9f9f9;
  border: 1px solid #ccc;
  border-radius: 8px;
  padding: 20px;
  width: 250px;
  box-shadow: 0 4px 8px rgba(0, 0, 0, 0.1);
  margin: 2px;
 
}
.container-card{
  display:flex;
  align-items: center;
}